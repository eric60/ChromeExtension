{
"name": "YouTube Scholar",
"version": "1.0",
"manifest_version": 2,
"description": "Need to block YouTube to stop procrastinating but still need to watch youtube tutorials
or lessons? Become a Youtube Scholar and only watch youtube videos in the educational categories!",
"icons": {
  "128": "icon.png"
},
"background": {
  "scripts": ["js/background.js"],
  "persistent": true
},
"browser_action": {
  "default_title": "YouTube Scholar",
  "default_popup": "popup.html"
},
"content_security_policy":"script-src 'self' https://ajax.googleapis.com https://fonts.googleapis.com https://www.googleapis.com; object-src 'self'",
"permissions": [
  "notifications",
  "background",
  "<all_urls>",
  "webRequest",
  "webRequestBlocking",
  "tabs"
]
}
