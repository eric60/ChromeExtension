{
"name": "YouTube Scholar",
"version": "1.0",
"manifest_version": 2,
"description": "Block youtube videos not in educational categories",
"icons": {
  "128": "icon.png"
},
"background": {
  "scripts": ["js/background.js"],
  "persistent": true
},
"browser_action": {
  "default_title": "YouTube Scholar",
  "default_popup": "popup.html"
},
"content_security_policy": "script-src 'self' https://ajax.googleapis.com https://www.googleapis.com; object-src 'self'",
"permissions": [
  "notifications",
  "background",
  "<all_urls>",
  "webRequest",
  "webRequestBlocking",
  "tabs"
 ]
}
